<?xml version='1.0' encoding='utf-8'?>
<widget id="it.repl.eai-dist" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>EAI Mobile Application</name>
    <description>
        EAI Mobile Application
    </description>
    <author email="jdbriggs81@gmail.com" href="http://phonegap.com">
        Jacob Briggs
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="https://repl.it/@elipsion/EAIservices" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <gap:plugin name="cordova-plugin-email-composer" source="npm" version="0.8.3" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
